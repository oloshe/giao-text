Page{
  background: #f5f5f5;
}
.title{
  font-size: 35rpx;
  color: #999;
  margin: 20rpx;
}
.btn{
  min-width: 100rpx;
  text-align: center;
  padding: 10rpx 20rpx;
  border-radius: 10rpx;
  margin-left: 10rpx;
  color: #fff;
  background: #0081ff;
  font-weight: 600;
  font-size: 35rpx;
  font-family: SumHei;
}
.btn.hover{
  background: #ccc;
}
.icon{
  margin-right: 20rpx;
}
.custom-btn{
  color: #fff !important;
  background: #8dc63f !important;
  font-size: 40rpx !important;
  border: none !important;
}
.custom-hover{
  color: #000 !important;
  background: #fff !important;
}
.collapse{
  padding: 20rpx !important;
  background: #fff !important;
}